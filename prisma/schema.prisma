generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model List {
  id Int @id @default(autoincrement())

  product    String?
  sender     String?
  from       String?
  department String?

  status Int?

  createdAt DateTime? @default(now())
  updatedAt DateTime? @updatedAt
}
